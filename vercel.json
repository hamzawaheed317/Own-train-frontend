{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": "build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "NODE_ENV": "production"
  }
}
